{
  "name": "relay-app",
  "private": true,
  "version": "0.0.1",
  "description": "A test app using relay.",
  "main": "src/index.js",
  "scripts": {
    "start": "parcel index.html",
    "lint": "eslint --ext .jsx --ext .js src/**",
    "prettier": "prettier --write ./src/ ",
    "relay": "relay-compiler --src ./src --schema ./schema/schema.graphql --extensions js jsx",
    "graphql": "postgraphile --cors -c postgres:///test --export-schema-graphql ./schema/schema.graphql -a"
  },
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "dependencies": {
    "postgraphile": "^4.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-relay": "^1.7.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "graphql": "^14.1.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "relay-compiler": "^1.7.0"
  }
}
